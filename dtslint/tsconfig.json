{
    "compilerOptions": {
        "noEmit": true,
        "strict": true,
        "noImplicitReturns": false,
        "noUnusedLocals": false,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "target": "es5",
        "lib": ["es2015", "dom"],
        "moduleResolution": "node",
        "skipLibCheck": true,
        "typeRoots": []
    }
}